<% if (pages > 0) { %>
<ul class="pagination text-center" id="pagination-nav">
  <% if (current == 1) { %>
  <li class="page-item disabled"><a class="page-link">First</a></li>
  <% } else { %>
  <li class="page-item">
    <a class="page-link" onclick="fetData(1,'<%=home%>')">First</a>
  </li>
  <% } %> <% let i = (Number(current) > 5 ? Number(current) - 4 : 1) %> <% if (i
  !== 1) { %>
  <li class="page-item disabled"><a class="page-link">...</a></li>
  <% } %> <% for (; i <= (Number(current) + 4) && i <= pages; i++) { %> <% if (i
  == current) { %>
  <li class="page-item active"><a class="page-link"><%= i %></a></li>
  <% } else { %>
  <li class="page-item">
    <a class="page-link" onclick="fetData('<%= i %>','<%=home%>')"><%= i %></a>
  </li>
  <% } %> <% if (i == Number(current) + 4 && i < pages) { %>
  <li class="page-item disabled"><a class="page-link">...</a></li>
  <% } %> <% } %> <% if (current == pages) { %>
  <li class="page-item disabled"><a class="page-link">Last</a></li>
  <% } else { %>
  <li class="page-item">
    <a class="page-link" onclick="fetData('<%= pages %>','<%=home%>')">Last</a>
  </li>
  <% } %>
</ul>
<% } %>
